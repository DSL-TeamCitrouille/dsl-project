{
    "name": "dam-dam-language",
    "description": "The language specific package",
    "version": "0.0.1",
    "type": "module",
    "engines": {
        "node": ">=20.10.0",
        "npm": ">=10.2.3"
    },
    "files": [
        "out",
        "src"
    ],
    "main": "./out/index.js",
    "module": "./out/index.js",
    "exports": {
        ".": {
            "types": "./out/index.d.ts",
            "default": "./out/index.js"
        }
    },
    "typesVersions": {
        "*": {
            ".": [
                "out/index"
            ]
        }
    },
    "scripts": {
        "clean": "shx rm -fr *.tsbuildinfo out",
        "build": "echo 'No build step'",
        "build:clean": "npm run clean && npm run build",
        "langium:generate": "langium generate",
        "langium:watch": "langium generate --watch",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:run": "vitest run",
        "test:coverage": "vitest run --coverage"
    },
    "dependencies": {
        "langium": "~4.1.0"
    },
    "devDependencies": {
        "@vitest/ui": "^3.2.4",
        "langium-cli": "~4.1.0",
        "vitest": "^3.2.4"
    },
    "volta": {
        "node": "20.19.2",
        "npm": "10.8.2"
    }
}
